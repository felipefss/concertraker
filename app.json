{
  "expo": {
    "name": "concertraker",
    "slug": "concertraker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "myapp",
    "plugins": [
      "expo-router"
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "pan"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4ae8ebc7-b96d-49d3-a0a9-465268ad079f"
      }
    },
    "owner": "felipefss"
  }
}
